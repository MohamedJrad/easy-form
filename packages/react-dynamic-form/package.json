{
  "name": "@med-jrad/react-dynamic-form",
  "private": false,
  "version": "1.1.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup index.tsx --format esm,cjs --dts --external react",
    "clean": "rm -rf dist",
    "dev": "tsup index.tsx --format esm,cjs --watch --dts --external react",
    "storybook": "start-storybook -p 6006 --no-open",
    "build-storybook": "build-storybook",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "type-check": "vitest typecheck",
    "lint": "eslint \"src/**/*.+(ts|js|tsx|jsx)\" --fix",
    "format": "prettier \"src/**/*.+(ts|js|tsx)\" --write"
  },
  "dependencies": {
    "formik": "^2.2.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@med-jrad/react-form-fields": "*",
    "styled-components": "^5.3.6",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/builder-vite": "^0.2.5",
    "@storybook/react": "^6.5.13",
    "@storybook/testing-library": "^0.0.13",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@vitejs/plugin-react": "^2.2.0",
    "babel-loader": "^8.3.0",
    "jsdom": "^20.0.3",
    "typescript": "^4.6.4",
    "@med-jrad/tsconfig": "*",
    "vite": "^3.2.3",
    "vitest": "^0.25.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
