{
  "name": "@med-jrad/react-dynamic-form",
  "private": false,
  "license": "MIT",
  "version": "0.0.46",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup index.tsx --format esm,cjs --dts --external react",
    "clean": "rm -rf dist",
    "dev": "tsup index.tsx --format esm,cjs --watch --dts --external react",
    "storybook": "start-storybook -p 6006 --no-open",
    "build-storybook": "build-storybook",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "type-check": "vitest typecheck",
    "lint": "eslint \"src/**/*.+(ts|js|tsx|jsx)\" --fix",
    "format": "prettier \"src/**/*.+(ts|js|tsx)\" --write"
  },
  "dependencies": {
    "@med-jrad/react-form-fields": "*",
    "formik": "^2.2.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.6",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@storybook/addon-actions": "^6.5.14",
    "@storybook/addon-essentials": "^6.5.14",
    "@storybook/addon-interactions": "^6.5.14",
    "@storybook/addon-links": "^6.5.14",
    "@storybook/builder-vite": "^0.2.5",
    "@storybook/react": "^6.5.14",
    "@storybook/testing-library": "^0.0.13",
    "@swc/core": "^1.3.22",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.46.0",
    "@vitejs/plugin-react": "^2.2.0",
    "babel-loader": "^8.3.0",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.31.11",
    "jsdom": "^20.0.3",
    "typescript": "*",
    "vite": "^3.2.3",
    "vitest": "^0.25.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
