{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React Library",
  "compilerOptions": {
    "composite": false,
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "inlineSources": false,
    "isolatedModules": true,
    "moduleResolution": "node",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "preserveWatchOutput": true,
    "skipLibCheck": true,
    "strict": true,
    "lib": [
      "ES2015",
      "DOM"
    ],
    "jsx": "react-jsx",
    "module": "ESNext",
    "target": "es6",
    "allowJs": true,
    "declarationDir": "./types",
    "emitDeclarationOnly": false,
    "incremental": true,
    "tsBuildInfoFile": "/home/mohamed/Documents/Dev/med-jrad/easy-form/node_modules/vitest/dist/tsconfig.tmp.tsbuildinfo"
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}